{
  "name": "bigquery-mcp-server",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "mkdir -p dist && bun build --compile --outfile=dist/$npm_package_name index.ts",
    "show-package-name": "echo $npm_package_name",
    "start": "./run-server.sh --project-id=${PROJECT_ID:-your-project-id}",
    "test": "cd test && ./test-server.js"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@google-cloud/bigquery": "^7.5.0",
    "@modelcontextprotocol/sdk": "^1.6.0",
    "zod": "^3.22.4"
  }
}
